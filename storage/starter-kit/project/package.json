{
  "name": "kawkab-app",
  "description": "kawkab project",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf .dist && tsc && cross-env NODE_NO_WARNINGS=1 cross-env NODE_ENV=production node .dist/index.js",
    "start": "cross-env NODE_NO_WARNINGS=1 cross-env NODE_ENV=production node .dist/index.js",
    "remove": "rimraf .dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "kawkab": "cross-env NODE_NO_WARNINGS=1 cross-env NODE_ENV=cli ts-node cli.ts"
  },
  "keywords": [
    "kawkab",
    "framework",
    "nodejs",
    "bunjs",
    "javascript",
    "js",
    "typescript",
    "ts",
    "backend",
    "api"
  ],
  "author": "Hassan Kerdash",
  "license": "MIT",
  "devDependencies": {
    "kawkabjs": "1.0.0-alpha.1",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
